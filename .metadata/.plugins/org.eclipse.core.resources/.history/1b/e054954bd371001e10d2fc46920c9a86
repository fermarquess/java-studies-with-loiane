	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Digite sua altura: ");
		double altura = scan.nextDouble();
		
		System.out.println("A sua altura Ã©: " + altura);
		
	}

}
